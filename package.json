{
  "name": "easy-co",
  "version": "1.0.5",
  "main": "dist/index.js",
  "bin": {
    "eco": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "nodemon --exec ts-node src/index.ts",
    "clean": "rm -rf dist",
    "cli": "npm run build && node dist/index.js",
    "link": "npm run build && npm link",
    "unlink": "npm unlink -g",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cli",
    "typescript",
    "commander"
  ],
  "author": "anand-ochiruranchimeg",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anand-ochiruranchimeg/easy-co.git"
  },
  "bugs": {
    "url": "https://github.com/anand-ochiruranchimeg/easy-co/issues"
  },
  "homepage": "https://github.com/anand-ochiruranchimeg/easy-co#readme",
  "dependencies": {
    "@types/inquirer": "^9.0.8",
    "colors": "^1.4.0",
    "commander": "^14.0.0",
    "inquirer": "^12.6.3",
    "inquirer-fuzzy-path": "^2.3.0",
    "shelljs": "^0.10.0",
    "table": "^6.9.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/inquirer-fuzzy-path": "^2.3.9",
    "@types/node": "^24.0.3",
    "@types/shelljs": "^0.8.16",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "description": "A simple CLI tool built with TypeScript and Commander.js"
}
